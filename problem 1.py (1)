# Conditional Tests

car = 'subaru'
print("Is car == 'subaru'? I predict True.")
print(car == 'subaru')  # True

print("\nIs car == 'audi'? I predict False.")
print(car == 'audi')  # False

age = 20
print("\nIs age >= 18? I predict True.")
print(age >= 18)  # True

print("\nIs age < 18? I predict False.")
print(age < 18)  # False

fruit = 'apple'
print("\nIs fruit != 'banana'? I predict True.")
print(fruit != 'banana')  # True

print("\nIs fruit == 'Apple'? I predict False.")
print(fruit == 'Apple')  # False (case-sensitive)

temperature = 30
print("\nIs temperature > 25? I predict True.")
print(temperature > 25)  # True

print("\nIs temperature < 10? I predict False.")
print(temperature < 10)  # False